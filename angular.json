{
  "version": 1,
  "projects": {
    "einkauf-api": {
      "root": "apps/einkauf-api",
      "sourceRoot": "apps/einkauf-api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/einkauf-api",
            "main": "apps/einkauf-api/src/main.ts",
            "tsConfig": "apps/einkauf-api/tsconfig.app.json",
            "assets": ["apps/einkauf-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/einkauf-api/src/environments/environment.ts",
                  "with": "apps/einkauf-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "einkauf-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/einkauf-api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/einkauf-api"],
          "options": {
            "jestConfig": "apps/einkauf-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": ["einkauf", "nest"]
    },
    "jk-comp": {
      "projectType": "application",
      "root": "apps/jk-comp",
      "sourceRoot": "apps/jk-comp/src",
      "prefix": "jan-krueger.eu",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/jk-comp",
            "index": "apps/jk-comp/src/index.html",
            "main": "apps/jk-comp/src/main.ts",
            "polyfills": "apps/jk-comp/src/polyfills.ts",
            "tsConfig": "apps/jk-comp/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/jk-comp/src/favicon.ico",
              "apps/jk-comp/src/assets"
            ],
            "styles": ["apps/jk-comp/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/jk-comp/src/environments/environment.ts",
                  "with": "apps/jk-comp/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "jk-comp:build:production"
            },
            "development": {
              "browserTarget": "jk-comp:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "jk-comp:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/jk-comp/src/**/*.ts",
              "apps/jk-comp/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/jk-comp"],
          "options": {
            "jestConfig": "apps/jk-comp/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "jk-comp-e2e": {
      "root": "apps/jk-comp-e2e",
      "sourceRoot": "apps/jk-comp-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/jk-comp-e2e/cypress.json",
            "devServerTarget": "jk-comp:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "jk-comp:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/jk-comp-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["jk-comp"]
    },
    "jk-comps": {
      "projectType": "library",
      "root": "libs/jk-comps",
      "sourceRoot": "libs/jk-comps/src",
      "prefix": "jk",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/jk-comps"],
          "options": {
            "project": "libs/jk-comps/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/jk-comps/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/jk-comps/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/jk-comps"],
          "options": {
            "jestConfig": "libs/jk-comps/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/jk-comps/src/**/*.ts",
              "libs/jk-comps/src/**/*.html",
              "libs/jk-comps/button/**/*.ts",
              "libs/jk-comps/button/**/*.html",
              "libs/jk-comps/input/**/*.ts",
              "libs/jk-comps/input/**/*.html"
            ]
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/jk-comps/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/jk-comps",
            "config": {
              "configFolder": "libs/jk-comps/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      },
      "tags": []
    },
    "jk-comps-e2e": {
      "root": "apps/jk-comps-e2e",
      "sourceRoot": "apps/jk-comps-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/jk-comps-e2e/cypress.json",
            "devServerTarget": "jk-comps:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "jk-comps:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/jk-comps-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["jk-comps"]
    }
  }
}
